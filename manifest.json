{
  "manifest_version": 3,
  "name": "IBM Autofill Extension",
  "version": "1.0",
  "description": "Autofills IBM Job Portal",
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["resume/*"],
      "matches": ["<all_urls>"]
    }
  ]
}
