{
  "manifest_version": 3,
  "name": "Candidate Autofill Extension",
  "version": "1.0",
  "description": "Autofills candidate name, mobile, education and repetitive data.",
  "permissions": ["storage", "scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
